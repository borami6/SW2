#define PIN_LED 7
unsigned int toggle;

int toggle_state(int toggle) {
  return !toggle;
}

void setup() {
  pinMode(PIN_LED, OUTPUT);
  toggle = 1; 
  
}

void loop() {
  digitalWrite(PIN_LED, 0);
  delay(1000);
  for(int i = 0; i <= 5; i +=1) {
    toggle = toggle_state(toggle);
    digitalWrite(PIN_LED, toggle);
    delay(100);
    toggle = toggle_state(toggle);
    digitalWrite(PIN_LED, toggle);
    delay(100);
  }
  exit(0);
  }
